package linkedList;

public class LinkedList {
	Node head;


	public Node insertAtTheEnd(int data){
		Node newNode=new Node(data);
		if(head==null){
			head=newNode;
		}
		else{
			Node temp=head;
			while(temp.next!=null){
				temp=temp.next;
			}
			temp.next=newNode;
		}

		return head;
	}

	public Node insertAtTheBeginning(int data){

		Node newNode=new Node(data);
		if(head!=null)
			newNode.next=head;

		head=newNode;


		return head;
	}
	public Node searchByValue(int val){
		if(head==null){
			return null;
		}
			else{			
			Node temp=head;			
			while(temp.next!=null){
				if(temp.data==val){
					return temp;
				}
				temp=temp.next;						
			}
			if(temp.data==val){
				return temp;
			}
			else{
				return null;
			}
		}
	}

public Node deleteByValue(int val){
	if(head==null){
		return null;
		
	}
	else if(head.next==null){
		if(head.data==val){
			head = null;
					}
		
		
	}
	
	
	
	
}











	public static void main(String [] args){






	}

}
